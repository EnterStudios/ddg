<ROSETTASCRIPTS>
        <SCOREFXNS>
	   <fa_talaris2014 weights=talaris2014/>
	   <fa_talaris2013_cart weights=talaris2013_cart/>
	   <fa_ddg_monomer weights=ddg_monomer/>
	   <fa_score12 weights=score12/>
	   <fa_soft_rep weights=soft_rep/>
	   <fa_talaris2013 weights=talaris2013/>
	   <fa_talaris2014_soft_fa_rep weights=talaris2014>
	     <Reweight scoretype="fa_rep" weight=0.055/>
	   </fa_talaris2014_soft_fa_rep>
	   <fa_talaris2014_soft_fa_dun weights=talaris2014>
	     <Reweight scoretype="fa_dun" weight=0.07/>
	   </fa_talaris2014_soft_fa_dun>
	   <fa_interface weights=interface/>
        </SCOREFXNS>

        <TASKOPERATIONS>
	  <ReadResfile name=rrf filename="%%pathtoresfile%%"/>
	  <ReadResfile name=rrf_pack filename="%%pathtopackresfile%%"/>
        </TASKOPERATIONS>

        <MOVERS>
	  <MinMover name=minmover scorefxn=fa_%%currentminmoverscorefxn%%  chi=1 bb=1 jump=ALL cartesian=%%cartminintbool%% type=lbfgs_armijo tolerance=0.0001 max_iter=2000 />
	  <ddG name=ddgmover scorefxn=fa_%%currentrepackscorefxn%% chain_num=%%chainstomove%% repack_unbound=%%repackunbound%% repack_bound=%%repackbound%% relax_bound=%%relaxbool%% task_operations=rrf_pack relax_mover=minmover />
        </MOVERS>

        <FILTERS>
	  <DdGScan name=alascan task_operations=rrf repeats=%%numberscanrepeats%% scorefxn=fa_%%currentscorefxn%% report_diffs=1 write2pdb=1 ddG_mover=ddgmover />
        </FILTERS>

	<MOVERS>
	  <ReportToDB name=%%PDBPosID%% batch_description="%%currentscorefxn%%">
	    <feature name=ResidueFeatures/>
	    <feature name=PdbDataFeatures/>
	    <feature name=ScoreTypeFeatures/>
	    <feature name=ScoreFunctionFeatures scorefxn=fa_%%currentscorefxn%%/>
	    <feature name=StructureScoresFeatures scorefxn=fa_%%currentscorefxn%%/>
	    <feature name=ResidueScoresFeatures scorefxn=fa_%%currentscorefxn%%/>
	    <feature name=DdGFeatures ddG_scan_mover=alascan/>
	  </ReportToDB>
	</MOVERS>

        <APPLY_TO_POSE>
        </APPLY_TO_POSE>

        <PROTOCOLS>
	  <Add mover_name=%%PDBPosID%%/>
        </PROTOCOLS>

        <OUTPUT />
</ROSETTASCRIPTS>
